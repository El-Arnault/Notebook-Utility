{
    "name": "notebook-utility",
    "version": "1.2.0",
    "description": "A simplistic note tracker with cli",
    "main": "build/index.js",
    "scripts": {
        "start": "node build/index.js",
        "build": "yarn run flow-remove-types source/ -- -d build/",
        "prepublish": "yarn run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/paper-lark/notebook-utility.git"
    },
    "keywords": [
        "note",
        "notebook",
        "command line",
        "terminal"
    ],
    "author": "Max Zhuravsky",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/paper-lark/notebook-utility/issues"
    },
    "bin": {
        "notes": "./build/index.js"
    },
    "homepage": "https://github.com/paper-lark/notebook-utility#readme",
    "devDependencies": {
        "babel-eslint": "^8.1.0",
        "eslint": "^4.12.1",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-flowtype": "^2.39.1",
        "eslint-plugin-node": "^5.2.1",
        "eslint-plugin-prettier": "^2.4.0",
        "flow": "^0.2.3",
        "flow-remove-types": "^1.2.3",
        "prettier": "^1.9.2",
        "prettier-eslint": "^8.6.0"
    },
    "dependencies": {
        "blessed": "^0.1.81"
    }
}
